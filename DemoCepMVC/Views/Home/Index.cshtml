@model IEnumerable<CepViewModel>

@{
    ViewData["Title"] = "CEPs";
}

<div class="text-center">
    <h2 class="display-4">Listagem: </h2>
    <table class="table table-striped">
        <thead>
            <tr>
                <th>CEP</th>
                <th>Logradouro</th>
                <th>Bairro</th>
                <th>Cidade</th>
                <th>Estado</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var cep in Model)
            {
                <tr>
                    <td>@cep.Cep</td>
                    <td>@cep.Logradouro</td>
                    <td>@cep.Bairro</td>
                    <td>@cep.Cidade</td>
                    <td>@cep.Estado</td>
                </tr>
            }
        </tbody>
    </table>

</div>
